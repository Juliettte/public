  JOURNAL = {{Astronomy and Astrophysics - A\\&A}},
  PUBLISHER = {{EDP Sciences}},
  VOLUME = {600},
  PAGES = {A35},
  YEAR = {2017},
  MONTH = Apr,
  DOI = {10.1051/0004-6361/201629470},
  KEYWORDS = {helioseismology ; solar physics ; numerical methods},
