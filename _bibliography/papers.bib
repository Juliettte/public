@article{tournemenne:hal-01963674,
  TITLE = {{A comparison of a one-dimensional finite element method and the transfer matrix method for the computation of wind music instrument impedance}},
  AUTHOR = {Tournemenne, Robin and Chabassier, Juliette},
  URL = {https://hal.inria.fr/hal-01963674},
  JOURNAL = {{Acta Acustica united with Acustica}},
