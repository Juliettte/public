  KEYWORDS = {piano modeling ; time domain ; nonlinear strings ; nonlinear precursor ; phantom partials ; energy preserving schemes},
  PDF = {https://hal.inria.fr/hal-00873089/file/Piano-model-revis.pdf},
  HAL_ID = {hal-00873089},
