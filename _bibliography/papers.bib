  DOI = {10.1016/j.amc.2019.04.065},
  KEYWORDS = {Helioseismology ; Equivalent conditions ; Boundary layer ; Multiscale expansions},
  PDF = {https://hal.inria.fr/hal-02320521/file/ChDuPe19.pdf},
  HAL_ID = {hal-02320521},
